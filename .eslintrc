{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
      "semi": [0],
      "quotes": [0],
      "no-irregular-whitespace": [0],
      "strict": [2, "global"],
      "no-use-before-define": 2,
      "no-unreachable": 2,
      "no-undef": 2,
      "no-unused-vars": 2,
      "no-shadow": 2,
      "valid-jsdoc": 2,
      "no-invalid-regexp": 2,
      "no-extra-parens": 2,
      "prefer-template": 2

  }
}
